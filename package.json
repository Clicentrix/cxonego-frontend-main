{
  "name": "CxOneGo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node --max-old-space-size=4096 build.cjs",
    "build:ts": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npx tsc -p tsconfig.prod.json --skipLibCheck --noEmit && cross-env NODE_OPTIONS=--max-old-space-size=4096 npx vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node server.cjs",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:start:production": "pm2 start ecosystem.config.cjs --env production",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs cxonego-frontend",
    "pm2:monitor": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "check-dist": "node -e \"if(!require('fs').existsSync('./dist')) { console.log('Building project...'); require('child_process').execSync('npm run build', {stdio: 'inherit'}); }\"",
    "postinstall": "npm run check-dist"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.4",
    "@capacitor/cli": "^5.7.4",
    "@capacitor/core": "^5.7.4",
    "@capacitor/ios": "^5.7.4",
    "@capacitor/push-notifications": "^5.0.6",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@firebase/messaging": "^0.12.9",
    "@mui/material": "^5.15.16",
    "@mui/styled-engine": "^5.15.14",
    "@mui/x-data-grid": "^7.3.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/firebase": "^3.2.1",
    "@types/react-big-calendar": "^1.8.8",
    "@types/react-google-recaptcha": "^2.1.9",
    "antd": "^5.7.0",
    "axios": "^1.4.0",
    "chart.js": "^4.4.2",
    "chartjs-plugin-datalabels": "^2.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.12.1",
    "install": "^0.13.0",
    "npm": "^10.8.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.6",
    "react-chartjs-2": "^5.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-google-recaptcha": "^3.1.0",
    "react-phone-number-input": "^3.4.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.0",
    "sass": "^1.85.1",
    "slick-carousel": "^1.8.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/core": "^1.11.22",
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "sass-loader": "^14.2.1",
    "typescript": "^4.9.5",
    "vite": "^4.4.5"
  }
}
